<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <ScrollView
        android:id="@+id/scroll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.constraint.ConstraintLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/textView13"
                android:layout_width="119dp"
                android:layout_height="68dp"
                android:layout_marginLeft="-5dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Display2"
                tools:text="4"
                android:gravity="center"
                app:layout_constraintLeft_toRightOf="@+id/textView9"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView9"
                app:layout_constraintTop_toTopOf="@+id/textView9"
                android:layout_marginTop="0dp"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="119dp"
                android:layout_height="68dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Display2"
                tools:text="101"
                android:gravity="center"
                app:layout_constraintLeft_toRightOf="@+id/textView6"
                android:layout_marginLeft="-2dp"
                app:layout_constraintTop_toTopOf="@+id/textView6"
                android:layout_marginTop="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView6"
                android:layout_marginBottom="8dp"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textViewSaltLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="-1dp"
                tools:text="Salt"
                android:textAppearance="@style/TextAppearance.AppCompat.Caption"
                app:layout_constraintLeft_toLeftOf="@+id/textView9"
                android:layout_marginBottom="2dp"
                app:layout_constraintBottom_toTopOf="@+id/textView9" />

            <TextView
                tools:text="Product Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewProductName"
                android:textAppearance="@style/TextAppearance.AppCompat.Display1"
                android:textColor="@color/colorPrimary"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp"
                tools:layout_constraintTop_creator="1"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                tools:layout_constraintLeft_creator="1"/>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="C"
                android:textAppearance="@style/TextAppearance.AppCompat.Display4"
                app:layout_constraintLeft_toRightOf="@+id/textViewProductName"
                android:layout_marginLeft="0dp"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="-16dp"
                app:layout_constraintTop_toBottomOf="@+id/textViewProductName"
                app:layout_constraintHorizontal_bias="0.508" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="250 Cal"
                android:textAppearance="@style/TextAppearance.AppCompat.Display2"
                tools:layout_editor_absoluteY="86dp"
                tools:layout_editor_absoluteX="45dp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="fff"
                tools:layout_editor_absoluteX="45dp"
                tools:layout_editor_absoluteY="488dp" />

            <TextView
                android:id="@+id/textViewSugarLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="Sugar"
                android:textAppearance="@style/TextAppearance.AppCompat.Caption"
                android:layout_marginLeft="0dp"
                app:layout_constraintLeft_toLeftOf="@+id/textView6"
                android:layout_marginBottom="2dp"
                app:layout_constraintBottom_toTopOf="@+id/textView6" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="119dp"
                android:layout_height="68dp"
                tools:text="56"
                android:textAppearance="@style/TextAppearance.AppCompat.Display2"
                android:gravity="center"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="263dp" />

            <TextView
                android:id="@+id/textViewFatLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="Fat"
                android:textAppearance="@style/TextAppearance.AppCompat.Caption"
                android:layout_marginLeft="0dp"
                app:layout_constraintLeft_toLeftOf="@+id/textView13"
                android:layout_marginBottom="2dp"
                app:layout_constraintBottom_toTopOf="@+id/textView13" />

            <Button
                android:id="@+id/buttonFindAlt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/find_alternatives"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="70dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/textView9"
                app:layout_constraintVertical_bias="0.791"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="parent" />


        </android.support.constraint.ConstraintLayout>
    </ScrollView>
</layout>